{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/bootstrap-material-design/dist/css/bootstrap-material-design.css",
        "assets/sass/odin-material-theme.scss"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/arrive/minified/arrive.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/bootstrap-material-design/dist/js/material.js",
        "../node_modules/bootstrap-material-design/dist/js/ripples.js",
        "../node_modules/three/build/three.js",
        "../node_modules/three-vrcontrols/VRControls.js",
        "../node_modules/three-vreffect/VREffect.js",
        "../node_modules/webvr-polyfill/build/webvr-polyfill.js",
        "../scripts/webvr-manager.js",
        "../node_modules/three/examples/js/loaders/ColladaLoader.js",
        "../scripts/reticulum.js",
        "../scripts/rendering.js",
        "../scripts/scenebuilder.js",
        "../scripts/scene.js",
        "../scripts/myimptest.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "serve": {
      "port": 6606
    },
    "styleExt": "css",
    "component": {}
  }
}
